  0% { transform: translateY(0px); opacity: 1; }
  75% { transform: translateY(20px); opacity: 0; }
}

@keyframes fade-down { 
  0% { transform: translateY(0px); opacity: 1; }
  75% { transform: translateY(20px); opacity: 0; }
}

@-webkit-keyframes tada { 
  0%, 100% { transform: scale3d(1, 1, 1); }
  10%, 20% { transform: scale3d(0.95, 0.95, 0.95) rotate3d(0, 0, 1, -10deg); }
  30%, 50%, 70%, 90% { transform: scale3d(1, 1, 1) rotate3d(0, 0, 1, 10deg); }
  40%, 60%, 80% { transform: scale3d(1, 1, 1) rotate3d(0, 0, 1, -10deg); }
}

